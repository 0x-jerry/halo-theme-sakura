  "scripts": {
    "dev": "ts-node server/index.ts",
    "build": "run-p build:*",
    "build:client": "run-p vite:*",
    "build:server": "rm -rf dist && tsc -p server",
    "start": "export NODE_ENV=production && node ./dist/index.js",
    "vite:client": "vite build --ssrManifest --outDir dist-vite/client",
    "vite:server": "vite build --ssr client/entry-server.ts --outDir dist-vite/server",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "glib release",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "yarn lint:fix"
  },
